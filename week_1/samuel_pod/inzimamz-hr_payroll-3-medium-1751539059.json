{
  "env": "hr_payroll",
  "model_provider": "openai",
  "model": "o4-mini",
  "num_trials": 3,
  "temperature": 1,
  "interface_num": 3,
  "task": {
    "user_id": "aud_settlement_task",
    "instruction": "The organization 231470fb-9185-4987-b403-932abc01638a no longer accepts AUD currency. As an HR administrator, you want to list all active workers under that organization. For each worker whose current contract is billed in AUD and is still active, fetch their contract summary, then process a final reimbursement request of 300 AUD with the reason 'Final expenses'. Next, issue a final settlement invoice of 5000 AUD due on 2025-06-24. Finally, terminate each of those contracts.",
    "actions": [
      {
        "name": "list_active_workers",
        "arguments": {
          "organization_id": "231470fb-9185-4987-b403-932abc01638a"
        }
      },
      {
        "name": "get_worker_contract_summary",
        "arguments": {
          "worker_id": "796ec069-0551-45a4-947f-89eb2f7d1c7a"
        }
      },
      {
        "name": "process_reimbursement_request",
        "arguments": {
            "worker_id": "796ec069-0551-45a4-947f-89eb2f7d1c7a",
            "amount": 300,
            "reason": "Final expenses"
        }
      },
      {
        "name": "create_new_invoice",
        "arguments": {
          "contract_id": "41849834-562e-42d7-8f79-4c9df369d60b",
          "amount": 5000,
          "currency": "AUD",
          "due_date": "2025-06-24"
        }
      },
      {
        "name": "terminate_worker_contract",
        "arguments": {
          "contract_id": "41849834-562e-42d7-8f79-4c9df369d60b"
        }
      }
    ],
    "outputs": [
      "796ec069-0551-45a4-947f-89eb2f7d1c7a",
      "41849834-562e-42d7-8f79-4c9df369d60b",
      "231470fb-9185-4987-b403-932abc01638a"
    ],
    "edges": [
      {
        "from": "instruction",
        "to": "list_active_workers",
        "connection": {
          "input": "organization_id",
          "output": "organization_id"
        }
      },
      {
        "from": "list_active_workers",
        "to": "get_worker_contract_summary",
        "connection": {
          "input": "worker_id",
          "output": "worker_id"
        }
      },
      {
        "from": "get_worker_contract_summary",
        "to": "process_reimbursement_request",
        "connection": {
            "input": "worker_id",
            "output": "worker_id"
        }
      },
      {
          "from": "instruction",
          "to": "process_reimbursement_request",
          "connection": {
              "input": "amount, reason",
              "output": "amount, reason"
          }
      },
      {
        "from": "get_worker_contract_summary",
        "to": "create_new_invoice",
        "connection": {
          "input": "contract_id",
          "output": "contract_id"
        }
      },
      {
        "from": "instruction",
        "to": "create_new_invoice",
        "connection": {
          "input": "amount, currency, due_date",
          "output": "amount, currency, due_date"
        }
      },
      {
        "from": "get_worker_contract_summary",
        "to": "terminate_worker_contract",
        "connection": {
          "input": "contract_id",
          "output": "contract_id"
        }
      }
    ],
    "num_edges": 7
  }
}
