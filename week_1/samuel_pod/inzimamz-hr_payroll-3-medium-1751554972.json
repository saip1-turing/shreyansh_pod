{
  "env": "hr_payroll",
  "model_provider": "openai",
  "model": "o4-mini",
  "num_trials": 3,
  "temperature": 1,
  "interface_num": 3,
  "task": {
    "user_id": "gbp_incentive_reimb_task",
    "instruction": "As an HR administrator, I am managing an update for worker incentives and reimbursements for the organization 011729d2-c96a-4bb7-8cb1-2ea3396fee5e. The organization is offering an incentive of 100 GBP and a reimbursement of 200 GBP to all workers who receive payments in GBP and have banking details that support GBP transfers, to cover currency exchange handling charges. One such worker, with user_id: fc64e513-c86a-4d9d-b6d6-a55edeaeb747, is currently active within the organization and has submitted updated banking information to be reflected: account_number = GB35GYCH87858197220697, bank_name = Warner-Kemp, curreny = GBP. Proceed to update this workers banking details to support GBP payments, then apply the 200 GBP reimbursement with reason 'banking support GBP', and issue the 100 GBP incentive invoice due July 30, 2025",
    "actions": [
        {
            "name": "list_active_workers",
            "arguments": {
                "organization_id": "011729d2-c96a-4bb7-8cb1-2ea3396fee5e"
            }
        },
        {
            "name": "get_worker_contract_summary",
            "arguments": {
                "worker_id": "0f87cfe5-b251-4fc5-a897-d8363207c484"
            }
        },
        {
            "name": "update_worker_bank_info",
            "arguments": {
                "worker_id": "0f87cfe5-b251-4fc5-a897-d8363207c484",
                "bank_info": {
                    "account_number": "GB35GYCH87858197220697",
                    "bank_name": "Warner-Kemp",
                    "currency": "GBP"
                }
            }
        },
        {
            "name": "process_reimbursement_request",
            "arguments": {
              "worker_id": "0f87cfe5-b251-4fc5-a897-d8363207c484",
              "amount": 200,
              "reason": "banking support GBP"
            }
        },
        {
            "name": "create_new_invoice",
            "arguments": {
              "contract_id": "eaaf202d-c788-4a0f-9ef6-3f990d631a4e",
              "amount": 100,
              "currency": "GBP",
              "due_date": "2025-07-30"
            }
        }
        ],
        "outputs": [
            "011729d2-c96a-4bb7-8cb1-2ea3396fee5e",
            "0f87cfe5-b251-4fc5-a897-d8363207c484",
            "eaaf202d-c788-4a0f-9ef6-3f990d631a4e"
        ],
        "edges": [
            {
              "from": "instruction",
              "to": "list_active_workers",
              "connection": {
                "input": "organization_id",
                "output": "organization_id"
              }
            },
            {
              "from": "list_active_workers",
              "to": "get_worker_contract_summary",
              "connection": {
                "input": "worker_id",
                "output": "worker_id"
              }
            },
            {
              "from": "get_worker_contract_summary",
              "to": "update_worker_bank_info",
              "connection": {
                "input": "worker_id",
                "output": "worker_id"
              }
            },
            {
              "from": "instruction",
              "to": "update_worker_bank_info",
              "connection": {
                "input": "bank_info.account_number, bank_info.bank_name, bank_info.currency",
                "output": "account_number, bank_name, currency"
              }
            },
            {
              "from": "get_worker_contract_summary",
              "to": "process_reimbursement_request",
              "connection": {
                "input": "worker_id",
                "output": "worker_id"
              }
            },
            {
              "from": "instruction",
              "to": "process_reimbursement_request",
              "connection": {
                "input": "amount, reason",
                "output": "amount, reason"
              }
            },
            {
              "from": "get_worker_contract_summary",
              "to": "create_new_invoice",
              "connection": {
                "input": "contract_id",
                "output": "contract_id"
              }
            },
            {
              "from": "instruction",
              "to": "create_new_invoice",
              "connection": {
                "input": "amount, currency, due_date",
                "output": "amount, currency, due_date"
              }
            }
        ],
        "num_edges": 8
    }
}
