                }
            },
            {
                "from": "fetch_time_summary_by_team",
                "to": "approve_overtime_entry",
                "connection": {
                    "input": "time_entries.time_entry_id",
                    "output": "time_entry_id"
                }
            },
            {
                "from": "create_new_contract",
                "to": "get_pending_reimbursements",
                "connection": {
                    "input": "contracts.user_id",
                    "output": "payments"
                }
            },
            {
                "from": "instruction",
                "to": "get_pending_reimbursements",
                "connection": {
                    "input": "status",
                    "output": "payments"
                }
            },
            {
                "from": "get_pending_reimbursements",
                "to": "check_user_virtual_cards",
                "connection": {
                    "input": "payments.user_id",
                    "output": "virtual_cards"
                }
            },
            {
                "from": "check_user_virtual_cards",
                "to": "block_virtual_card",
                "connection": {
                    "input": "virtual_cards.card_id",
                    "output": "virtual_cards"
                }
            },
            {
                "from": "block_virtual_card",
                "to": "upload_document",
                "connection": {
                    "input": "virtual_cards.user_id",
                    "output": "documents"
                }
            },
            {
                "from": "instruction",
                "to": "upload_document",
                "connection": {
                    "input": "document_type, title",
                    "output": "documents"
                }
            },
            {
                "from": "upload_document",
                "to": "update_document_status",
                "connection": {
                    "input": "documents.document_id",
                    "output": "documents"
                }
            },
            {
                "from": "instruction",
                "to": "update_document_status",
                "connection": {
                    "input": "new_status",
                    "output": "documents"
                }
            }
        ],
        "num_edges": 15
    }
}
