{
  "env": "enterprise_wiki",
  "model_provider": "openai",
  "model": "o4-mini",
  "num_trials": 3,
  "temperature": 1,
  "interface_num": 2,
  "task": {
      "user_id": "1",
      "instruction": "You are Charles Johnson (email: charles.johnson@enterprise.biz). Sarah Garcia (email: sarah.garcia@corp.net) has requested help with community engagement. She wants to comment that 'Documentation is very clear and well-structured' on the API Documentation page in the Engineering space, but first needs to understand the existing discussion. She also wants to set up notifications for the Engineering space. Help her by first identifying her user profile, then showing her the existing comments on the API Documentation page, creating her comment about the documentation clarity, and setting up space watching for Engineering. Finally, create a notification to inform her that her comment was posted successfully.",
      "actions": [
          {
              "name": "get_user_by_email",
              "arguments": {
                  "email": "sarah.garcia@corp.net"
              },
              "output": {
                  "id": 2
              }
          },
          {
              "name": "get_spaces_by_filters",
              "arguments": {
                  "name": "Engineering"
              },
              "output": [
                  {
                      "id": 3
                  }
              ]
          },
          {
              "name": "get_space_pages",
              "arguments": {
                  "space_id": 3
              },
              "output": [
                  {
                      "id": 176
                  },
                  {
                      "id": 583
                  }
              ]
          },
          {
              "name": "get_page_comments",
              "arguments": {
                  "page_id": 176
              },
              "output": [350, 434]
          },
          {
              "name": "create_comment",
              "arguments": {
                  "page_id": 176,
                  "content": "Documentation is very clear and well-structured",
                  "created_by": 2
              },
              "output": {
                  "id": "new_comment_id"
              }
          },
          {
              "name": "create_watcher",
              "arguments": {
                  "user_id": 2,
                  "target_type": "space",
                  "target_id": 3
              },
              "output": {
                  "id": "new_watcher_id"
              }
          },


          {
              "name": "create_notification",
              "arguments": {
                  "user_id": 2,
                  "notification_type": "page_commented",
                  "title": "New page commented notification",
                  "message": "Your comment on the API Documentation page has been posted successfully.",
                  "created_by": 2,
                  "target_type": "comment",
                  "target_id": "new_comment_id"
              },
              "output": {
                  "id": "new_notification_id"
              }
          },

          {
              "name": "get_user_notifications",
              "arguments": {
                  "user_id": 2
              },
              "output": [
                  {
                      "id": "new_notification_id"
                  }
              ]
          }
      ],
      "outputs": [
          "2",
          "3",
          "176"
      ],
      "edges": [
          {
              "from": "instruction",
              "to": "get_user_by_email",
              "connection": {
                  "output": "email",
                  "input": "email"
              }
          },

          {
              "from": "instruction",
              "to": "get_spaces_by_filters",
              "connection": {
                  "output": "name",
                  "input": "name"
              }
          },
          {
              "from": "get_spaces_by_filters",
              "to": "get_space_pages",
              "connection": {
                  "output": "output[0].id",
                  "input": "space_id"
              }
          },
          {
              "from": "get_space_pages",
              "to": "get_page_comments",
              "connection": {
                  "output": "output[0].id",
                  "input": "page_id"
              }
          },
          {
              "from": "instruction",
              "to": "create_comment",
              "connection": {
                  "output": "content",
                  "input": "content"
              }
          },
          {
              "from": "get_user_by_email",
              "to": "create_comment",
              "connection": {
                  "output": "id",
                  "input": "created_by"
              }
          },
          {
              "from": "get_space_pages",
              "to": "create_comment",
              "connection": {
                  "output": "output[0].id",
                  "input": "page_id"
              }
          },
          {
              "from": "get_user_by_email",
              "to": "create_watcher",
              "connection": {
                  "output": "id",
                  "input": "user_id"
              }
          },
          {
              "from": "instruction",
              "to": "create_watcher",
              "connection": {
                  "output": "target_type",
                  "input": "target_type"
              }
          },
          {
              "from": "get_spaces_by_filters",
              "to": "create_watcher",
              "connection": {
                  "output": "output[0].id",
                  "input": "target_id"
              }
          },

          {
              "from": "instruction",
              "to": "create_notification",
              "connection": {
                  "output": "title, notification_type, message, target_type",
                  "input": "title, notification_type, message, target_type"
              }
          },
          {
              "from": "get_user_by_email",
              "to": "create_notification",
              "connection": {
                  "output": "id",
                  "input": "user_id"
              }
          },
          {
              "from": "get_user_by_email",
              "to": "create_notification",
              "connection": {
                  "output": "id",
                  "input": "created_by"
              }
          },
          {
              "from": "create_comment",
              "to": "create_notification",
              "connection": {
                  "output": "id",
                  "input": "target_id"
              }
          },

          {
              "from": "get_user_by_email",
              "to": "get_user_notifications",
              "connection": {
                  "output": "id",
                  "input": "user_id"
              }
          }
      ],
      "num_edges": 15
  }
}