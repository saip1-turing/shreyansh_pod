{
    "env": "enterprise_wiki",
    "model_provider": "openai",
    "model": "o4-mini",
    "num_trials": 3,
    "temperature": 1,
    "interface_num": 2,
    "task": {
        "user_id": "80",
        "instruction": "You are Yuki Lewis (email: yuki.lewis@systems.io), and you are a community manager helping users engage with the wiki system. Deborah White (email: deborah.white@tech.org) wants to participate in community discussions. She wants to find the 'User Manual - Part 558' page in the Engineering space, add a helpful comment saying 'This documentation is very helpful and well-structured. Thank you for providing such clear information' in markdown format, set up watching for the Engineering space to stay updated with notifications enabled, and create a notification to inform her that her comment was successfully posted via email. The notification should be titled 'New page commented notification' and have the following message in the body - 'Your comment on the User Manual - Part 558 page has been posted successfully.'. You also want to mark the notification as read.",
        "actions": [
            {
                "name": "get_user_by_email",
                "arguments": {
                    "email": "yuki.lewis@systems.io"
                },
                "output": {
                    "id": 80
                }
            },
            {
                "name": "get_user_by_email",
                "arguments": {
                    "email": "deborah.white@tech.org"
                },
                "output": {
                    "id": 189
                }
            },
            {
                "name": "get_spaces_by_filters",
                "arguments": {
                    "name": "Engineering"
                },
                "output": [
                    {
                        "id": 3
                    }
                ]
            },
            {
                "name": "get_space_pages",
                "arguments": {
                    "space_id": "3"
                },
                "output": [
                    {
                        "id": 583
                    }
                ]
            },
            {
                "name": "create_comment",
                "arguments": {
                    "page_id": 583,
                    "content": "This documentation is very helpful and well-structured. Thank you for providing such clear information.",
                    "created_by": 80,
                    "content_format": "markdown"
                },
                "output": {
                    "id": 801
                }
            },
            {
                "name": "create_watcher",
                "arguments": {
                    "user_id": 189,
                    "target_type": "space",
                    "target_id": 3,
                    "notifications_enabled": true
                },
                "output": {
                    "id": 501
                }
            },
            {
                "name": "create_notification",
                "arguments": {
                    "user_id": 189,
                    "notification_type": "page_commented",
                    "title": "New page commented notification",
                    "message": "Your comment on the User Manual - Part 558 page has been posted successfully.",
                    "created_by": 80,
                    "target_type": "comment",
                    "target_id": 801,
                    "delivery_method": "email"
                },
                "output": {
                    "id": 701
                }
            },
            {
                "name": "update_notification_read_status",
                "arguments": {
                    "notification_id": 701,
                    "is_read": true
                },
                "output": {
                    "status": "updated"
                }
            }
        ],
        "outputs": [
            "189",
            "3", 
            "583"
        ],
        "edges": [
            {
                "from": "instruction",
                "to": "get_user_by_email",
                "connection": {
                    "output": "email",
                    "input": "email"
                }
            },
            {
                "from": "instruction",
                "to": "get_spaces_by_filters",
                "connection": {
                    "output": "name",
                    "input": "name"
                }
            },
            {
                "from": "get_spaces_by_filters",
                "to": "get_space_pages",
                "connection": {
                    "output": "id",
                    "input": "space_id"
                }
            },
            {
                "from": "get_user_by_email",
                "to": "create_comment",
                "connection": {
                    "output": "id",
                    "input": "created_by"
                }
            },
            {
                "from": "get_space_pages",
                "to": "create_comment",
                "connection": {
                    "output": "id",
                    "input": "page_id"
                }
            },
            {
                "from": "instruction",
                "to": "create_comment",
                "connection": { 
                    "output": "content, content_format",
                    "input": "content, content_format"
                }
            },
            {
                "from": "get_user_by_email",
                "to": "create_watcher",
                "connection": {
                    "output": "id",
                    "input": "user_id"
                }
            },
            {
                "from": "instruction",
                "to": "create_watcher",
                "connection": {
                    "output": "target_type,notifications_enabled",
                    "input": "target_type,notifications_enabled"
                }
            },
            {
                "from": "get_spaces_by_filters",
                "to": "create_watcher",
                "connection": {
                    "output": "id",
                    "input": "target_id"
                }
            },

            {
                "from": "get_user_by_email",
                "to": "create_notification",
                "connection": {
                    "output": "id",
                    "input": "user_id"
                }
            },
            {
                "from": "get_user_by_email",
                "to": "create_notification",
                "connection": {
                    "output": "id",
                    "input": "created_by"
                }
            },
            {
                "from": "instruction",
                "to": "create_notification",
                "connection": {
                    "output": "title, notification_type, message, target_type, delivery_method",
                    "input": "title, notification_type, message, target_type, delivery_method"
                }
            },
            {
                "from": "create_comment",
                "to": "create_notification",
                "connection": {
                    "output": "id",
                    "input": "target_id"
                }
            },
            {
                "from": "create_notification",
                "to": "update_notification_read_status",
                "connection": {
                    "output": "notification_id",
                    "input": "notification_id"
                }
            },
            {
                "from": "instruction",
                "to": "update_notification_read_status",
                "connection": {
                    "output": "is_read",
                    "input": "is_read"
                }
            }
        ],
        "num_edges": 15
    }
}
