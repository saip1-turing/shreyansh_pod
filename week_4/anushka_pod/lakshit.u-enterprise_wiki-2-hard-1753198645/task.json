{
    "env": "enterprise_wiki",
    "model_provider": "openai",
    "model": "o4-mini",
    "num_trials": 3,
    "temperature": 1,
    "interface_num": 2,
    "task": {
        "user_id": "80",
        "instruction": "You are Yuki Lewis (email: yuki.lewis@systems.io), a community manager helping users engage with the wiki system. Deborah White (email: deborah.white@tech.org) wants to participate in community discussions. She needs to: 1) First verify her user profile, 2) Find the 'User Manual - Part 558' page in the Engineering space, 3) Review existing comments on that page, 4) Add a helpful comment about the documentation with markdown format, 5) Set up watching for the Engineering space to stay updated with notifications enabled, and 6) Let her create a notification to inform her that her comment was successfully posted. Help her complete this community engagement workflow step by step.",
        "actions": [
            {
                "name": "get_user_by_email",
                "arguments": {
                    "email": "yuki.lewis@systems.io"
                },
                "output": {
                    "id": 80
                }
            },
            {
                "name": "get_user_by_email",
                "arguments": {
                    "email": "deborah.white@tech.org"
                },
                "output": {
                    "id": 189
                }
            },
            {
                "name": "get_spaces_by_filters",
                "arguments": {
                    "name": "Engineering"
                },
                "output": [
                    {
                        "id": 3
                    }
                ]
            },
            {
                "name": "get_space_pages",
                "arguments": {
                    "space_id": "3"
                },
                "output": [
                    {
                        "id": 583
                    }
                ]
            },
            {
                "name": "get_page_comments",
                "arguments": {
                    "page_id": 583
                },
                "output": [400]
            },
            {
                "name": "create_comment",
                "arguments": {
                    "page_id": 583,
                    "content": "This documentation is very helpful and well-structured. Thank you for providing such clear information.",
                    "created_by": 189,
                    "content_format": "markdown"
                },
                "output": {
                    "id": 801
                }
            },
            {
                "name": "create_watcher",
                "arguments": {
                    "user_id": 189,
                    "target_type": "space",
                    "target_id": 3,
                    "notifications_enabled": true
                },
                "output": {
                    "id": 501
                }
            },
            {
                "name": "create_notification",
                "arguments": {
                    "user_id": 189,
                    "notification_type": "page_commented",
                    "title": "New page commented notification",
                    "message": "Your comment on the User Manual - Part 558 page has been posted successfully. You are now watching the Engineering space for updates.",
                    "created_by": 189,
                    "target_type": "comment",
                    "target_id": 801
                },
                "output": {
                    "id": 701
                }
            }
        ],
        "outputs": [
            "189",
            "3", 
            "583"
        ],
        "edges": [
            {
                "from": "instruction",
                "to": "get_user_by_email",
                "connection": {
                    "output": "email",
                    "input": "email"
                }
            },
            {
                "from": "instruction",
                "to": "get_spaces_by_filters",
                "connection": {
                    "output": "name",
                    "input": "name"
                }
            },
            {
                "from": "get_spaces_by_filters",
                "to": "get_space_pages",
                "connection": {
                    "output": "id",
                    "input": "space_id"
                }
            },
            {
                "from": "get_space_pages",
                "to": "get_page_comments",
                "connection": {
                    "output": "id",
                    "input": "page_id"
                }
            },

            {
                "from": "get_user_by_email",
                "to": "create_comment",
                "connection": {
                    "output": "id",
                    "input": "created_by"
                }
            },
            {
                "from": "get_space_pages",
                "to": "create_comment",
                "connection": {
                    "output": "id",
                    "input": "page_id"
                }
            },
            {
                "from": "instruction",
                "to": "create_comment",
                "connection": { 
                    "output": "content, content_format",
                    "input": "content, content_format"
                }
            },
            {
                "from": "get_user_by_email",
                "to": "create_watcher",
                "connection": {
                    "output": "id",
                    "input": "user_id"
                }
            },
            {
                "from": "instruction",
                "to": "create_watcher",
                "connection": {
                    "output": "target_type,notifications_enabled",
                    "input": "target_type,notifications_enabled"
                }
            },
            {
                "from": "get_spaces_by_filters",
                "to": "create_watcher",
                "connection": {
                    "output": "id",
                    "input": "target_id"
                }
            },

            {
                "from": "get_user_by_email",
                "to": "create_notification",
                "connection": {
                    "output": "id",
                    "input": "user_id"
                }
            },
            {
                "from": "get_user_by_email",
                "to": "create_notification",
                "connection": {
                    "output": "id",
                    "input": "created_by"
                }
            },
            {
                "from": "instruction",
                "to": "create_notification",
                "connection": {
                    "output": "title, notification_type, message, target_type",
                    "input": "title, notification_type, message, target_type"
                }
            },
            {
                "from": "create_comment",
                "to": "create_notification",
                "connection": {
                    "output": "id",
                    "input": "target_id"
                }
            }
        ],
        "num_edges": 14
    }
}
